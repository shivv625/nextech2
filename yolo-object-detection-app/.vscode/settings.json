{
    "python.pythonPath": "venv/bin/python",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.organizeImports": true
    },
    "eslint.enable": true,
    "eslint.validate": [
        "javascript",
        "javascriptreact"
    ],
    "files.exclude": {
        "**/__pycache__": true,
        "**/*.pyc": true
    }
}